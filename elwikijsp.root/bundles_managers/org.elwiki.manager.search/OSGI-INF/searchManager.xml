<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" activate="startup" name="org.apache.wiki.search.DefaultSearchManager">
   <implementation class="org.apache.wiki.search.DefaultSearchManager"/>
   <service>
      <provide interface="org.apache.wiki.api.search.SearchManager"/>
   </service>
   <reference bind="setWikiConfiguration" cardinality="1..1" interface="org.elwiki.configuration.IWikiConfiguration" name="IWikiConfiguration" policy="static"/>
   <reference bind="setReferenceManager" cardinality="1..1" interface="org.apache.wiki.api.references.ReferenceManager" name="ReferenceManager" policy="dynamic"/>
   <reference bind="setPageManager" cardinality="1..1" interface="org.apache.wiki.pages0.PageManager" name="PageManager" policy="dynamic"/>
</scr:component>
