      <provide interface="org.apache.wiki.api.ui.CommandResolver"/>
   </service>
   <reference bind="setConfiguration" cardinality="1..1" interface="org.elwiki.configuration.IWikiConfiguration" name="IWikiConfiguration" policy="static"/>
   <reference bind="setPageManager" cardinality="0..1" interface="org.apache.wiki.pages0.PageManager" name="PageManager" policy="dynamic"/>
   <reference bind="setURLConstructor" cardinality="0..1" interface="org.apache.wiki.url0.URLConstructor" name="URLConstructor" policy="dynamic"/>
</scr:component>
