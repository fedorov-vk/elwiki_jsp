<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="org.apache.wiki.ui.DefaultCommandResolver">
   <implementation class="org.apache.wiki.ui.DefaultCommandResolver"/>
