<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" name="org.apache.wiki.pages.DefaultPageManager">
   <implementation class="org.apache.wiki.pages.DefaultPageManager"/>
   <service>
      <provide interface="org.apache.wiki.pages0.PageManager"/>
   </service>
   <reference bind="setWikiConfiguration" cardinality="1..1" interface="org.elwiki.configuration.IWikiConfiguration" name="IWikiConfiguration" policy="static"/>
   <reference bind="setAclManager" cardinality="1..1" interface="org.apache.wiki.auth.acl.AclManager" name="AclManager" policy="static"/>
   <reference bind="setReferenceManager" cardinality="1..1" interface="org.apache.wiki.api.references.ReferenceManager" name="ReferenceManager" policy="static"/>
   <reference bind="setTasksManager" cardinality="1..1" interface="org.apache.wiki.api.tasks.TasksManager" name="TasksManager" policy="static"/>
   <reference bind="setDifferenceManager" cardinality="1..1" interface="org.apache.wiki.api.diff.DifferenceManager" name="DifferenceManager" policy="static"/>
</scr:component>
